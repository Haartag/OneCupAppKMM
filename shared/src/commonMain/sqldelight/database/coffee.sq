CREATE TABLE coffeeEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    description  TEXT NOT NULL,
    template INTEGER NOT NULL,
    isFavourite INTEGER NOT NULL,
    created INTEGER NOT NULL
);

getAllCoffee:
SELECT *
FROM coffeeEntity;

insertCoffee:
INSERT OR REPLACE
INTO coffeeEntity (
    id,
    title,
    description,
    template,
    isFavourite,
    created
) VALUES (?,?,?,?,?,?);

